{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
       "virtualMachineName": {
             "value": "AzStackHost"
        },
       "virtualMachineSize": {
            "value": "Standard_E32s_v3"
        },
        "dataDiskSizeinGB": {
            "value": 1024
        },
        "dataDiskCount": {
            "value": 8
        },
        "virtualNetworkName": {
            "value": "AzureStack-VNET"
        },
        "adminUsername": {
            "value": "<VM Local Admin>"
        },
        "adminPassword": {
            "value": "<VM Password>"
        },
        "addressPrefix": {
            "value": "10.0.0.0/24"
        },
        "subnetName": {
            "value": "default"
        },
        "subnetPrefix": {
            "value": "10.0.0.0/24"
        },
        "publicIpAddressType": {
            "value": "Dynamic"
        },
        "publicDnsName": {
            "value": "azstackdev"
        },
        "AzureADTenant": {
            "value": "<tenant>.onmicrosoft.com"
        },
        "AzureADGlobalAdmin": {
            "value": "<account>@<tenant>.onmicrosoft.com"
        },
        "AzureADGlobalAdminPassword": {
            "value": "<Password>"
        },
        "enableRDSH": {
            "value": false
        },
        "siteLocation": {
            "value": "<Azure Region>"
        }
    }
}
 
